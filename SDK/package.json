{
    "name": "eip808-sdk",
    "version": "1.0.0",
    "description": "npm module that allow request on the BlockChain Ethereum using BTU protocol (EIP808)",
    "main": "./cjs/index.js",
    "module": "./esm/index.js",
    "directories": {
	"test": "test"
    },
    "scripts": {
	"build-ressources": "cd ressources && rm -rf build/ && ./../node_modules/.bin/truffle deploy && cd ..",
	"build-module": "babel esm --out-dir cjs",
	"build": "npm run build-ressources && npm run build-module",
	"prepublishOnly": "npm run build"
    },
    "babel": {
	"presets": [
	    "es2015",
	    "stage-2"
	],
	"plugins": [
	    [
		"transform-runtime",
		{
		    "polyfill": false,
		    "regenerator": true
		}
	    ]
	]
    },
    "repository": {
	"type": "git",
	"url": "git+https://github.com/LalosBastien/node-eip808-sdk.git"
    },
    "keywords": [
	"Ethereum",
	"BlockChain",
	"SDK",
	"BTU",
	"EIP808"
    ],
    "author": "Lalos Bastien",
    "license": "Apache-2.0",
    "bugs": {
	"url": "https://github.com/LalosBastien/node-eip808-sdk/issues"
    },
    "homepage": "https://github.com/LalosBastien/node-eip808-sdk#readme",
    "dependencies": {
	"eth-lightwallet": "^3.0.1",
	"ethereumjs-abi": "^0.6.5",
	"ethereumjs-tx": "^1.3.3",
	"ethereumjs-util": "^5.1.4",
	"ipfs-api": "^18.1.1",
	"joi": "^13.1.2",
	"solc": "^0.4.20",
	"web3": "^1.0.0-beta.30",
	"truffle-hdwallet-provider": "0.0.3"
    },
    "devDependencies": {
	"babel-cli": "^6.24.1",
	"babel-plugin-transform-runtime": "^6.23.0",
	"babel-preset-es2015": "^6.24.1",
	"babel-preset-stage-2": "*",
	"truffle": "^4.0.6"
    }
}
